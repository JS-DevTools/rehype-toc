{
  "name": "rehype-toc",
  "version": "1.0.0-alpha.0",
  "description": "A rehype plugin that adds a table of contents (TOC) to the page",
  "keywords": [
    "unified",
    "rehype",
    "plugin",
    "rehype-plugin",
    "table-of-contents",
    "toc",
    "html"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://jsdevtools.org/rehype-toc",
  "repository": {
    "type": "git",
    "url": "https://github.com/JS-DevTools/rehype-toc.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage lib",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "mocha && npm run lint",
    "coverage": "nyc mocha",
    "upgrade": "npm-check -u",
    "bump": "bump --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm run build && npm test && npm run bump"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/command-line-args": "^5.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "@types/unist": "^2.0.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-modular": "^7.0.0",
    "mocha": "^6.1.4",
    "npm-check": "^5.9.0",
    "nyc": "^14.1.1",
    "rehype-parse": "^6.0.0",
    "rehype-stringify": "^6.0.0",
    "shx": "^0.3.2",
    "tslint": "^5.17.0",
    "tslint-modular": "^1.4.1",
    "typescript": "^3.5.1",
    "typescript-tslint-plugin": "^0.5.0",
    "unified": "^7.1.0",
    "version-bump-prompt": "^5.0.3"
  },
  "dependencies": {}
}
